{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
      {
        "matchPackagePatterns": ["^eslint"],
        "groupName": "eslint packages"
      },
      {
        "matchUpdateTypes": ["minor", "patch"],
        "labels": ["UPDATE-MONOR-PATCH"]
      }
  ]
}
